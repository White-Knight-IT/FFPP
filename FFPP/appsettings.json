{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "ApiSettings": {
    "CachePath": "/root/ffpp/cache",
    "PersistentPath": "/root/ffpp/persistent",
    "WebRootPath": "/var/www/ffpp-webfront",
    "HttpsRedirect": true,
    "ShowSwaggerUi": true,
    "RunSwagger": true,
    "ServeStaticFiles": false,
    "ShowDevEndpoints": false,
    "WebUiUrl": "https://localhost",
    "DnsProvider": "quad9",
    "DbSettings": {
      "MysqlUser": "ffppapiservice",
      "MysqlPassword": "wellknownpassword",
      "MysqlServer": "localhost",
      "MysqlServerPort": "7704"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:7074"
      },
      "Http": {
        "Url": "http://localhost:7073"
      }
    }
  },
  "AllowedHosts": "*"
}